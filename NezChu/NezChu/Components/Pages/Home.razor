@page "/"
@inject IConfiguration Config
@inject NezChuDbContext _dbContext

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<p class="h4">
    Default con: @defaultCon
</p>

<p class="h4">
    DbContext Con: @dbcon
</p>

@code {
    private string defaultCon = "";
    private string dbcon = "";
    private string res = "";

    protected override async Task OnInitializedAsync()
    {
        defaultCon = Config.GetConnectionString("SupabaseConnectionString") ?? "unknown";

        dbcon = _dbContext.Database.GetConnectionString() ?? "unknown";
    }
}